<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/2019/option.xsd">
	<import>
		<categories>
			<category name="show.supportThread">
				<parent>show</parent>
			</category>
		</categories>
		<options>
			<option name="show_support_thread_single_board">
				<categoryname>show.supportThread</categoryname>
				<optiontype>boolean</optiontype>
				<defaultvalue>1</defaultvalue>
				<enableoptions>show_support_thread_board_id,show_support_thread_categories</enableoptions>
				<showorder>1</showorder>
			</option>
			<option name="show_support_thread_board_id">
				<categoryname>show.supportThread</categoryname>
				<optiontype>WBBBoardSelect</optiontype>
				<showorder>2</showorder>
			</option>
			<option name="show_support_thread_categories">
				<categoryname>show.supportThread</categoryname>
				<optiontype>showCategoryMultiSelect</optiontype>
				<showorder>3</showorder>
			</option>
		</options>
	</import>
</data>
